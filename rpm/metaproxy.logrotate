/var/log/metaproxy.log {
	weekly
	missingok
	rotate 4
	compress
	delaycompress
	notifempty
	prerotate
		if [ -f /var/run/metaproxy.pid ]; then
			/sbin/service metaproxy configtest > /dev/null
		fi
	endscript
	postrotate
		if [ -f /var/run/metaproxy.pid ]; then
			/sbin/service metaproxy reload > /dev/null
		fi
	endscript
}
