<?xml version="1.0"?>
<!-- Uses dummy backend + query rewrite filter.. -->
<metaproxy xmlns="http://indexdata.com/metaproxy" version="1.0">
  <start route="start"/>
  <filters>
    <filter id="frontend" type="frontend_net">
      <port>@:9000</port>
    </filter>
  </filters>
  <routes>
    <route id="start">
      <filter refid="frontend"/>
      <filter type="log">
        <message>F</message>
	<time-format>%Y%m%d-%H%M%S</time-format>
      </filter>
      <filter type="query_rewrite">
       <xslt stylesheet="../etc/pqf2pqf.xsl"/>
       <charset to="iso-8859-1"/>
      </filter>
      <filter type="log">
        <message>B</message>
	<time-format>%Y%m%d-%H%M%S</time-format>
	<category init-options="true" request-session="true" response-session="true"
		  request-apdu="true" response-apdu="true"
		  />
      </filter>
      <filter type="backend_test"/>
      <filter type="bounce"/>
    </route>
  </routes>
</metaproxy>

