<yp2 xmlns="http://indexdata.dk/yp2/config/1">
  <start route="start"/>
  <filters>
  </filters>
  <routes>
    <route id="start">
      <filter type="frontend_net">
        <threads>10</threads>
        <port>@:9000</port>
      </filter>
      <filter type="log">
        <message>F</message>
      </filter>
      <filter type="multi">
        <virtual>
          <target>localhost:9900</target>
          <target>localhost:9901</target>
        </virtual>
        <virtual>
          <vhost>multi</vhost>
          <target>localhost:9900</target>
          <target>localhost:9901</target>
        </virtual>
      </filter>
      <filter type="log">
        <message>B</message>
      </filter>
      <filter type="z3950_client">
        <timeout>30</timeout>
      </filter>
    </route>
  </routes>
</yp2>

