<!-- $Id: config0.xml,v 1.4 2006-11-29 13:00:54 marc Exp $ -->
<metaproxy xmlns="http://indexdata.com/metaproxy" version="1.0">
  <start route="start"/>
  <filters>
    <filter id="frontend" type="frontend_net">
      <port>@:9000</port>
    </filter>
  </filters>
  <routes>  
    <route id="start">
      <filter refid="frontend"/>
      <filter type="log"/>
      <filter type="bounce"/>
    </route>
  </routes>
</metaproxy>

